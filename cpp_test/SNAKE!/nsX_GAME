int print(char *s)
{
    if(!s)
        return 0;
    while(*s)
        write(1,s++,1);
    return 1;
}
int print(int s)
{
    int i = 0;
    char r[100];

    r[99] = '\0';
    if(!s)
        return 0;
    while(s)
    {
        r[98-i++] = s % 10 + 48;
        s /= 10;
    }
    char *rr = &r[99-i];
    print(rr);
    return 1;
}
int main()
{
    float alldelta = 0;
    float delta = 0;
    OnStart();
    while(1)
    {
        delta = alldelta - delta;
        OnUpdate(delta);
        alldelta += 1;
    }
    return 0;
}